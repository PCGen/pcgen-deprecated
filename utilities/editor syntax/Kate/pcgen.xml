		<contexts>
			<context attribute="Normal Text" lineEndContext="Normal" name="Normal" >
				<RegExpr attribute="Comment" String="^#.*$" />
				<keyword String="tags" attribute="Tags" />
				<keyword String="tagswithyesno" attribute="Tags" context="TagsWithYN" />
				<keyword String="adjustments" attribute="Tags" context="Adjusters" />
				<keyword String="colortext" attribute="Tags" context="Descriptive" />
				<RegExpr String="SOURCE[\w]*" attribute="Tags" context="Descriptive" />
				<keyword String="enforcers" attribute="Tags" context="Enforce" />
				<StringDetect String="ABILITY" attribute="Tags" context="ABILITY" />
				<StringDetect String="ASPECT" attribute="Tags" context="ASPECT" />
				<StringDetect String="BONUS" context="BONUS" />
				<StringDetect String="DEFINE" attribute="Tags" context="DEFINE" />
				<StringDetect String="NATURALATTACKS" attribute="Tags" context="NatAttacks" />
				<StringDetect String="SPELLS" attribute="Tags" context="SPELLS" />
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
			</context>
			<context name="KeepNeat" >
				<RegExpr context="Normal" String="[\t]" />
				<StringDetect String="Prestige" context="#stay" />
				<keyword String="modifiers" attribute="Altered" />
				<RegExpr String="[!]*PRE[\w]*" attribute="PRETags" context="PRExxx" />
				<AnyChar String="]/=,.()[+*%!:^{}-]" attribute="Normal Text" />
				<RegExpr attribute="Dice" String="[\d]+d[\d]+" />
				<keyword String="typers" attribute="Tags" context="Typing"/>
				<keyword Context="Altered" string="modifiers" />
				<Int attribute="Numbers" />
				<keyword attribute="Numbers" String="globalvariables" />
			</context>
			<context name="ABILITY" attribute="Sub-Tags" lineEndContext="Normal" >
				<DetectChar char="=" attribute="Normal" context="#pop" />
				<StringDetect String="NORMAL" attribute="Sub-Tags" context="Listing" />
				<StringDetect String="AUTOMATIC" attribute="Sub-Tags" context="Listing" />
				<StringDetect String="VIRTUAL" attribute="Sub-Tags" context="Listing" />
				<DetectChar char="|" attribute="Pipes" />
				<IncludeRules context="KeepNeat" />
			</context>
			<context name="Adjusters" attribute="Keywords" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
			</context>
			<context name="Altered" attribute="Altered" lineEndContext="Normal" >
				<RegExpr context="Normal" String="[\t]" />
			</context>
			<context name="ASPECT" attribute="Assignment" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Descriptive" />
			</context>
			<context name="BONUS" lineEndContext="Normal" attribute="Tags">
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Tags2Sub" />
			</context>
			<context name="DEFINE" attribute="Assignment" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Variables" />
			</context>
			<context name="Descriptive" attribute="Descrpitive" lineEndContext="Normal" >
				<DetectString String="CASTERLEVEL" attribute="Numbers" />
				<DetectChar char="|" attribute="Pipes" context="Variables" />
				<RegExpr String="[%][\d]*" attribute="Sub-Tags" />
				<IncludeRules context="KeepNeat" />
			</context>
			<context name="Enforce" attribute="Tags" lineEndContext="Normal">
				<IncludeRules context="KeepNeat" />
				<StringDetect String="NUMCHOICES" attribute="Tags" context="Typing" />
				<StringDetect String="ABILITY" attribute="Tags" context="ABILITY" />
				<StringDetect String="ADDCHOICE" attribute="Tags" context="Listing" />
				<StringDetect String="CHOOSE" attribute="Tags" context="Listing" />
				<DetectChar char="|" attribute="Pipes" context="Listing" />
			</context>
			<context name="Listing" attribute="Assignment" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
			</context>
			<context name="NatAttacks" attribute="Assignment" lineEndContext="Normal" >
				<DetectChar char="," attribute="Normal" context="Typing" />
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
			</context>
			<context name="PRExxx" attribute="Normal Text" lineEndContext="Normal" >
				<Int attribute="Numbers" context="Normal" />
				<DetectChar char="Q" attribute="Sub-Tags" />
				<RegExpr String="[\w]" context="Normal" />
				<IncludeRules context="KeepNeat" />
			</context>
			<context name="SPELLS" attribute="Descrpitive" lineEndContext="Normal" >
				<StringDetect String="TIMEUNIT" attribute="Tags" context="Typing" />
				<StringDetect String="TIMES" attribute="Tags" context="Typing" />
				<StringDetect String="CASTERLEVEL" attribute="Tags" context="Typing" />
				<DetectChar char="|" attribute="Pipes" />
				<IncludeRules context="KeepNeat" />
			</context>
			<context name="Tags2Sub" attribute="Sub-Tags" lineEndContext="Normal" >
				<DetectChar char="|" attribute="Pipes" context="Variables" />
				<IncludeRules context="KeepNeat" />
			</context>
			<context name="TagsWithYN" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<keyword String="yesno" attribute="Sub-Tags" context="Normal" />
			</context>
			<context name="Typing" attribute="Keywords" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="#pop" />
			</context>
			<context name="Variables" attribute="Variables" lineEndContext="Normal" >
				<keyword string="globalvariables"  attribute="Numbers" />
				<RegExpr string="COUNT\[[\w]+([\W][\w]+)*\]" attribute="Numbers" />
				<RegExpr String="WEIGHT\.[\w]+" attribute="Numbers" />
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
			</context>
		</contexts>
		<itemDatas>
			<itemData name="Normal Text" defStyleNum="dsNormal" />
			<itemData name="Tags" defStyleNum="dsKeyword" />
			<itemData name="PRETags" defStyleNum="dsAlert"/>
			<itemData name="Sub-Tags" defStyleNum="dsDataType" />
			<itemData name="Assignment" defStyleNum="dsString" />
			<itemData name="Pipes" defStyleNum="dsFloat" />
			<itemData name="Numbers" defStyleNum="dsDecVal" />
			<itemData name="Comment" defStyleNum="dsComment" />
			<itemData name="Descrpitive" defStyleNum="dsComment" />
			<itemData name="Variables" defStyleNum="dsDecVal" italic="1" />
			<itemData name="Altered" defStyleNum="dsError" />
			<itemData name="Keywords" defStyleNum="dsOthers" />
			<itemData name="Dice" defStyleNum="dsBaseN" />
		</itemDatas>
